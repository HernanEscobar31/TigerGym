<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".altas">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="REGISTRAR USUARIOS"
        android:textColor="#E6AD00"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="40dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="89dp"
        android:layout_height="72dp"
        app:srcCompat="@drawable/gym" />

    <EditText
        android:id="@+id/password"
        android:layout_width="323dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPassword"
        android:textColor="@color/white"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="281dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Usuario:"
        android:textColor="#E6AD00"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="163dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Nombre Completo:"
        android:textColor="#E6AD00"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="329dp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Correo:"
        android:textColor="#E6AD00"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="505dp" />

    <EditText
        android:id="@+id/usuario"
        android:layout_width="323dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/white"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Edad:"
        android:textColor="#E6AD00"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="427dp" />

    <EditText
        android:id="@+id/idus"
        android:layout_width="319dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/white"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="116dp" />

    <EditText
        android:id="@+id/correo"
        android:layout_width="319dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/white"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="536dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="ID Usuario:"
        android:textColor="#E6AD00"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="92dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Password:"
        android:textColor="#E6AD00"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="249dp" />

    <EditText
        android:id="@+id/nombre"
        android:layout_width="323dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/white"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="361dp" />

    <EditText
        android:id="@+id/edad"
        android:layout_width="323dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/white"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="449dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="158dp"
        android:layout_height="48dp"
        android:backgroundTint="#E6AD00"
        android:onClick="altasUsuarios"
        android:text="Registrar"
        android:textSize="20sp"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="615dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="158dp"
        android:layout_height="50dp"
        android:backgroundTint="#E6AD00"
        android:onClick="cerrar"
        android:text="Salir"
        android:textSize="20sp"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="614dp" />

</androidx.constraintlayout.widget.ConstraintLayout>